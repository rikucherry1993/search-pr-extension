{
    "manifest_version": 3,
    "name": "PR Search",
    "version": "0.1",
    "description": "Add options to github PR and issues search",
    "icons": {
        "16": "icons/icon.png",
        "48": "icons/icon.png",
        "128": "icons/icon.png"
    },
    "content_scripts": [
        {
            "js": [
                "src/content.js"
            ],
            "matches": [
                "*://*.github.com/*"
            ]
        }
    ]
}